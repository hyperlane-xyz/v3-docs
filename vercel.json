{
  "buildCommand": "yarn install && yarn build",
  "outputDirectory": "build",
  "framework": "docusaurus-2",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--max_old_space_size=4096",
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "silent": true
  }
}
